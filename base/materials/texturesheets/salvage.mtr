material textures2/concrete/concrete_clean_13_env
{
	surfacetype "concrete"
	{
		diffusemap	picmip -1	textures2/concrete/concrete_clean_13_d.tga
		specularmap	picmip -2	textures2/concrete/concrete_clean_13_s.tga
		bumpmap 	picmip -1	textures2/concrete/concrete_clean_13_local.tga
		vertexcolor
		
		diffuseDetailMap		textures/detail/bump/metal_detail01_d.tga
		bumpDetailMap			textures/detail/bump/metal_detail01_local.tga
		specDetailMap			textures/detail/bump/metal_detail01_d.tga
		detailMult	5,5,0,0
	}
	{
		maskcolor
		map			makealpha(textures2/concrete/concrete_clean_13_s.tga)
	}
	{
		blend		gl_dst_alpha, gl_one
		maskalpha
		program		sfx/cubemap
		red			Parm0
		green   	Parm1
		blue		Parm2
	}
}

