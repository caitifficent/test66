material texturesheets/salvage/salvage_outdoor_2_noshadows_melted
{
	noshadows
	surfaceTypeMap "texturesheets/salvage/salvage_outdoor_2.stmap"
	{
		diffusemap	picmip -1	texturesheets/salvage/salvage_outdoor_2_d.tga
		specularmap	picmip -2	texturesheets/salvage/salvage_outdoor_2_s.tga
		bumpmap 	picmip -1	texturesheets/salvage/salvage_outdoor_2_local.tga
		vertexcolor
		
		diffuseDetailMap		textures/detail/bump/metal_detail01_d.tga
		bumpDetailMap			textures/detail/bump/metal_detail01_local.tga
		specDetailMap			textures/detail/bump/metal_detail01_d.tga
		detailMult	4,16,0,0
    }
    {
        diffusemap 	textures/sfx/lfwall27alava_d
		bumpmap 	textures/sfx/lfwall27alava_local
        specularmap	textures/sfx/lfwall27alava_s
        inverseVertexColor
        scale 2, 8
    }
    {
        blend		gl_one , gl_one
    	map		textures/sfx/radiogoop_red
    	rotate		time*.00015
    	inverseVertexColor
    	scale 4, 16
    }
    {
        blend		gl_one , gl_one
        map		textures/sfx/radiogoop_red
        rotate		time*-.00015
        translate	time*.002,1
    	inverseVertexColor
    	scale 4, 16
    }
}

