material texturesheets/salvage/salvage_decals_1
{
	qer_editorimage texturesheets/salvage/salvage_decals_1.tga
	polygonOffset
	sort decal
	
	nonsolid
 	translucent
	{
		program filterVertexColorCoverageFogged
 		blend filter
		map texturesheets/salvage/salvage_decals_1.tga
		maskAlpha
		vertexcolor
		maskDepth
		depthFunc lequal
	}
}

material texturesheets/salvage/salvage_outdoor_1
{
	surfaceTypeMap "texturesheets/salvage/salvage_outdoor_1.stmap"
	{
		diffusemap	picmip -1	texturesheets/salvage/salvage_outdoor_1_d.tga
		specularmap	picmip -2	texturesheets/salvage/salvage_outdoor_1_s.tga
		bumpmap 	picmip -1	texturesheets/salvage/salvage_outdoor_1_local.tga
		vertexcolor
	
		diffuseDetailMap		textures/detail/bump/metal_detail01_d.tga
		bumpDetailMap			textures/detail/bump/metal_detail01_local.tga
		specDetailMap			textures/detail/bump/metal_detail01_d.tga
		detailMult	2.5,20,0,0
	}
}

