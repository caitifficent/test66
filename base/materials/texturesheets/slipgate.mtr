material texturesheets/slipgate/snow_research_glass01
{
	noSelfShadow
	noshadows
	nonsolid
	twosided
	translucent
	useTemplate templates/windowglass_mask <
		texturesheets/slipgate/snow_research_glass01_d.tga,
		texturesheets/slipgate/snow_research_glass01_local.tga,
		texturesheets/slipgate/snow_research_glass01_mask.tga,
		2, 1.0, 0.3
	>
	{
		blend blend
		depthfunc less
		map texturesheets/slipgate/snow_research_glass01_dirt.tga
		
	}
}

