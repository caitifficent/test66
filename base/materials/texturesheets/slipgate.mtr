// ========================
// Village mapobjects
// ========================
material models/mapobjects/slipgate/slipgate_mapobjects01
{
	surfacetype "metal"
	{
		program	interaction/basic_detailwm
		diffusemap		picmip -1	models/mapobjects/slipgate/office/slipgate_mapobjects01_d.tga
		bumpmap			picmip -2	models/mapobjects/slipgate/office/slipgate_mapobjects01_local.tga
		specularmap		picmip -2	models/mapobjects/slipgate/office/slipgate_mapobjects01_s.tga
		vertexcolor

		diffuseDetailMap		textures/detail/bump/vehicle_detail01_d.tga
		specDetailMap			textures/detail/bump/vehicle_detail01_d.tga
		bumpDetailMap			textures/detail/bump/vehicle_detail01_local.tga
		detailMult 5,5,0,0
		detailWeightMap			models/mapobjects/slipgate/office/slipgate_mapobjects01_detailmask.tga
	}
}

material models/mapobjects/slipgate/slipgate_mapobjects01_noshadows
{
	noshadows
