// ========================
// GDF Base fort
// ========================
material texturesheets/slipgate/fort01
{
	surfaceTypeMap "texturesheets/slipgate/fort01.stmap"
	{
		diffusemap 		picmip -1	texturesheets/slipgate/fort01_d.tga
		specularmap 	picmip -2	texturesheets/slipgate/fort01_s.tga
		bumpmap 		picmip -1	texturesheets/slipgate/fort01_local.tga
		vertexcolor
	
		bumpDetailMap          textures/detail/bump/concrete_detail01_local.tga
		detailMult 3,12,0,0 
	}
}

