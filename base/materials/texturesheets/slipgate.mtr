material texturesheets/slipgate/fort04_decals
{
	polygonOffset 0.00525
	sort decal	
	nonsolid
 	translucent

	{
		program 		filterVertexColorCoverage
		blend 			filter
		map				picmip -1	texturesheets/slipgate/fort04_decals_d.tga
		maskAlpha
		vertexcolor
	}
}

