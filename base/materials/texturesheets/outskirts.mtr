material texturesheets/outskirts/outskirts_decals
{
	polygonOffset
	sort decal	
	nonsolid
 	translucent
	{
		program filterVertexColorCoverageFogged
 		blend filter
		map picmip -1	texturesheets/outskirts/outskirts_decals_d.tga
		maskAlpha
		vertexcolor
		maskDepth
		depthFunc lequal
	}
}

