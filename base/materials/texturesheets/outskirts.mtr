material texturesheets/outskirts/residential_wall1
{
	surfacetype "concrete"
	{
		diffusemap	picmip -1	texturesheets/outskirts/residential_wall1_d.tga
		specularmap	picmip -2	texturesheets/outskirts/residential_wall1_wet_s.tga
		bumpmap 	picmip -1	texturesheets/outskirts/residential_wall1_local.tga
		vertexcolor

		diffuseDetailMap		textures/detail/bump/metal_detail01_d.tga
		bumpDetailMap			textures/detail/bump/metal_detail01_local.tga
		specDetailMap			textures/detail/bump/metal_detail01_d.tga
		detailMult	2.5,20,0,0
	}
}

