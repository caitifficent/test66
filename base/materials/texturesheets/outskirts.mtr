// Windows
material texturesheets/outskirts/windows1
{
	surfacetype "glass"
	noshadows
	{
		diffusemap	texturesheets/outskirts/windows1_d.tga
		bumpmap		texturesheets/outskirts/windows1_local.tga
		vertexcolor
	}
	{
		blend blend
		program sfx/windowglass 
		diffusemap	texturesheets/outskirts/windows1_d.tga
		bumpmap		texturesheets/outskirts/windows1_local.tga
		parameters 2, 1.7, 0.6
	}
}

