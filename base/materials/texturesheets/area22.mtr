// DECALS

material texturesheets/area22/decals_cracksandmetal
{     
	{	
      	diffusemap	picmip -1	texturesheets/area22/decals_cracksandmetal_d.tga
		specularmap	picmip -2	texturesheets/area22/decals_cracksandmetal_s.tga
		bumpmap 	picmip -1	texturesheets/area22/decals_cracksandmetal_local.tga
		alphaTest 0.5
	}
}

material texturesheets/area22/decals_dirt
{
	polygonOffset
	sort decal	
	nonsolid
 	translucent
	{
		program	filterVertexColorCoverage
		blend	filter
		map						texturesheets/area22/decals_dirt_d.tga
		maskAlpha
		vertexcolor
	}
}

material texturesheets/area22/gradient_round
{
	polygonOffset 0.25
	sort decal	
	nonsolid
 	translucent
	{
 		program	filterVertexColorCoverage
 		blend	filter
