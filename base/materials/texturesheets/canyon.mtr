// DECALS

material texturesheets/canyon/can_decals_1
{
	polygonOffset
	sort decal
	noShadows
	nonsolid
	translucent
	{
		program filterVertexColor
		blend	filter
		map	texturesheets/canyon/can_decals_1.tga
		vertexColor
	}
}


// TEXTURESHEETS

material texturesheets/canyon/can_metal_wall_1
{
	surfaceTypeMap "texturesheets/canyon/can_metal_wall_1.stmap"
	{
		diffusemap	picmip -1	texturesheets/canyon/can_metal_wall_1_d.tga
		specularmap picmip -2	texturesheets/canyon/can_metal_wall_1_s.tga
		bumpmap 	picmip -1	texturesheets/canyon/can_metal_wall_1_local.tga
		
		diffuseDetailMap		textures/detail/bump/metal_detail01_d.tga
		specDetailMap			textures/detail/bump/metal_detail01_d.tga
		bumpDetailMap			textures/detail/bump/metal_detail01_local.tga
		detailMult	2.5,20,0,0
		vertexcolor
	}
}

