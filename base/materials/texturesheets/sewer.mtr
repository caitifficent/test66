material texturesheets/sewer/sewer_decals
{
	polygonOffset
	sort decal
	nonsolid
 	translucent
 	noshadows
 	{
 		program		filterCoverage
 		blend		filter
 		map 		texturesheets/sewer/sewer_decals_d.tga
 		maskAlpha
 	}
}

