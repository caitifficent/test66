material texturesheets/volcano/grating
{
	twosided
	nonsolid
	playerclip
	flyerhiveclip
	noimpact
	noshadows
	{
		//polygonOffset
		//qer_editorimage texturesheets/volcano/grating_d.tga
		diffusemap	picmipmin -1 maxLod 2	texturesheets/volcano/grating_d.tga
		specularmap    	picmip -1		texturesheets/volcano/grating_s.tga
		bumpmap 	picmip -1		texturesheets/volcano/grating_local.tga
		alphatest 	0.5
		vertexcolor
	}
	
}

