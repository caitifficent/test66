material texturesheets/island/docks
{
	surfaceTypeMap "texturesheets/island/docks"
	{
		diffusemap	picmip -1 texturesheets/island/docks_d.tga
		bumpmap		picmip -1 texturesheets/island/docks_local.tga
		specularmap	picmip -2 texturesheets/island/docks_s.tga

		diffuseDetailMap textures/detail/bump/metal_detail01_d
		bumpDetailMap textures/detail/bump/metal_detail01_local
		detailMult	4,32,0,0
		vertexcolor
	}
}

