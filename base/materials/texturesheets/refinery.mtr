// WINDOWS

material texturesheets/refinery/refinery_windows02
{
	diffusemap	picmip -1	texturesheets/refinery/refinery_windows02_d.tga
	bumpmap		picmip -1	texturesheets/refinery/refinery_windows02_local.tga
	surfacetype glass
	{
		blend blend
		program sfx/windowglass 
		diffusemap	picmip -1	texturesheets/refinery/refinery_windows02_d.tga
		bumpmap		picmip -1	texturesheets/refinery/refinery_windows02_local.tga
		parameters 2, 1.0, 0.3
	}
}

