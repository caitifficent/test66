// DECALS

material texturesheets/refinery/refinery_decals
{
	polygonOffset
	sort decal
	nonsolid
 	translucent
	{
		program	filterVertexColorCoverage
 		blend	filter
		map	texturesheets/refinery/refinery_decals_d.tga
		maskAlpha
		vertexcolor
		maskDepth
		depthFunc	lequal
	}
}

material texturesheets/refinery/building02b
{
	surfaceTypeMap	"texturesheets/refinery/building02b"
	{
		program	interaction/basic_detailwm	
		diffusemap	picmip -1	texturesheets/refinery/building02b_d.tga
		specularmap	picmip -2	texturesheets/refinery/building02b_s.tga
		bumpmap		picmip -1	texturesheets/refinery/building02b_local.tga
		
		diffuseDetailMap		textures/detail/bump/concrete_detail01_d.tga
		specDetailMap			textures/detail/bump/concrete_detail01_d.tga
		bumpDetailMap			textures/detail/bump/concrete_detail01_local.tga
		detailWeightMap			texturesheets/refinery/building02b_detailmask.tga
		detailMult	2.5,20,0,0
		
		vertexcolor
	}
}

