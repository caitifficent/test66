material textures/doors/biodome_door_melting
{
	surfacetype "metal"
	{ 
		diffusemap	picmip -1	textures/doors/biodome_door_d.tga
		specularmap	picmip -2	textures/doors/biodome_door_s.tga
		bumpmap		picmip -1	textures/doors/biodome_door_local.tga
		vertexColor
		
		diffuseDetailMap		textures/detail/bump/vehicle_detail01_d.tga
		bumpDetailMap			textures/detail/bump/vehicle_detail01_local.tga
		specDetailMap 			textures/detail/bump/vehicle_detail01_d.tga
		detailMult	6,6,0,0
	}
	{
		blend		add
		map			textures/sfx/mining_laser_melting.tga
		rotate		time*.0001
		translate	time*-.0001,time*.0001
		scale		2, 2
		inverseVertexColor
		rgb 0.75 // *** needs scripting for fade
	}
	{
		blend		add
		map			textures/sfx/mining_laser_melting.tga
		rotate		time*-.0001
		translate	time*.0001,0.5
		scale		3, 3
		inverseVertexColor
		rgb 0.75 // *** needs scripting for fade
	}
}