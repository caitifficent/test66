material textures2/doors/door_hangar1
{
	surfacetype "metal"
	{
		program interaction/basic_detailwm
		diffusemap				textures2/doors/door_hangar_1_d.tga
		specularmap	picmip -1	textures2/doors/door_hangar_1_s.tga
		bumpmap		picmip -1	textures2/doors/door_hangar_1_local.tga
		vertexcolor
		
		diffuseDetailMap		textures/detail/bump/metal_detail01_d.tga
		specDetailMap			textures/detail/bump/metal_detail01_d.tga
		bumpDetailMap			textures/detail/bump/metal_detail01_local.tga
		detailMult	11,11,0,0
		detailWeightMap			textures2/doors/door_hangar_1_detailmask.tga

	}
}

