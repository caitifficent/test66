material textures/sfx/flare_davepulse01
{
	noshadows
	translucent
	nonsolid
	discrete
	deform flare 	8//slowstart_one_zero [Parm4 + (time * (0.1 * Parm3)) ]
	qer_editorimage		textures/editor/flare.tga

	{
		blend		add
		map		_quadratic
		red 	( davepulse01 [Parm4 + (time * (0.1 * Parm3)) ]) * Parm0
		green 	( davepulse01 [Parm4 + (time * (0.1 * Parm3)) ]) * Parm1
		blue	( davepulse01 [Parm4 + (time * (0.1 * Parm3)) ]) * Parm2
		vertexcolor
		//maskAlpha
	}
}


