material textures/sfx/laser {
	noshadows
	translucent
	nonsolid
	{
		blend		blend
		map 		maxLod 2	makealpha(textures/particles/smoke/laser_smoke_drift2.tga)
		vertexcolor
		maskalpha
		useTemplate templates/materials/animate<16, .333>
	}
	{
		blend		add
		map 		maxLod 2	textures/particles/smoke/laser_smoke_drift2.tga
		vertexcolor
 		maskalpha
		useTemplate templates/materials/animate<16, .666>
	}


}

