material textures/strogg_walls/strogg_wall_01
{
	surfaceType 	"metal"
	qer_editorimage textures/strogg_walls/strogg_wall_01_d.tga
		{
			bumpmap    	picmip -1	textures/strogg_walls/strogg_wall_01_local.tga
			diffusemap 	picmip -1	textures/strogg_walls/strogg_wall_01_d.tga
			specularmap	picmip -2	textures/strogg_walls/strogg_wall_01_s.tga
			vertexcolor
			
			bumpDetailMap          textures/detail/bump/metal_detail01_local.tga
			detailMult 2,2,0.4,0.1			
		}
}

