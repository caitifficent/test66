material textures/base_trim/grate1ashiny
{
	surfaceType "metal"
	qer_editorimage	textures/base_trim/grate1a_shiny.tga
	noshadows
	twoSided
	{
		bumpmap models/seneca/textures/grate1_local.tga
		specularmap	picmip -2	models/seneca/textures/grate1a_s.tga
		diffusemap models/seneca/textures/grate1a_d.tga
		alphaTest 0.5
	}
}

