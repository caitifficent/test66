
#include <materials/commandmaps.include>


material commandmaps/broken {
	{
		map _white
		colored
		red 0.25
		green 0.25
		blue 0.25
		writeDepth
	}
	{
		blend		add
		translate 	time * jrad_statictable[ time * 5 ], time * jrad_statictable[ time * 5 ]
		rotate 		time * 7
		map    		textures/decals/greystatic.tga
		alpha 		0.1
		scale		parm7, parm7
	}
	
}


