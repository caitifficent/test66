material textures/military/floor_tiles1
{
	surfaceType "metal"
	{
		diffusemap	textures/military/floor_tiles1_d.tga
		specularmap 	textures/military/floor_tiles1_s.tga
		bumpmap 	textures/military/floor_tiles1_local.tga
		vertexColor
		
		diffuseDetailMap		textures/detail/bump/vehicle_detail01_d.tga
		bumpDetailMap			textures/detail/bump/vehicle_detail01_local.tga
		specDetailMap			textures/detail/bump/vehicle_detail01_d.tga
		detailMult	2,2,0,0
	}
}

