// Oil Pump

material models/mapobjects/oil_pump/pump

    {
    	surfaceType	"metal"
    	renderbump -size 1024 512 -aa 2 -colormap models/pump/pump_local.tga models/pump/hi.lwo
		noshadows

	 	diffusemap		models/mapobjects/oil_pump/pump_d.tga
    	specularmap		models/mapobjects/oil_pump/pump_s.tga
     	bumpmap 		addnormals (models/mapobjects/oil_pump/pump_local.tga, heightmap (models/mapobjects/oil_pump/pump_h.tga, 1))
    }