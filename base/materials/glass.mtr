material textures/glass/dirty_window
{
	noSelfShadow
	noshadows
	twosided
	translucent
	surfaceType "glass"
	forceoverlays
	sort decal

	{
		maskcolor
		map		makealpha(textures/glass/dirty_window_mask1.tga)
	}

	{
		blend		gl_dst_alpha, gl_one
		maskalpha
		program sfx/cubemap
		red		Parm0
		green   Parm1
		blue	Parm2
	}

	{
     	blend	filter
		map		textures/glass/dirty_window2.tga
	}
}

