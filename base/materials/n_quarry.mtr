///////////////////////////////////////
//
//	QUARRY PIPE CAP
//
///////////////////////////////////////


material textures/nerve/quarry_pipecap2a
{
 	noShadows
	nonsolid
	noimpact
	twoSided
	qer_editorimage textures/nerve/quarry_pipecap2a_d.tga

	{       
		diffusemap 	textures/nerve/quarry_pipecap2a_d.tga
		specularmap	textures/decals/pipecap2a_s.tga
		bumpmap 	addnormals (textures/decals/pipecap2a_local.tga, heightmap (textures/decals/pipecap2_bmp.tga, 1 ))      	 
		alphaTest 0.5
	}

}

