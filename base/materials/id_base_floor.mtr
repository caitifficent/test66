material textures/base_floor/a_coldstairs_04_fin
{
	surfaceType "metal"
	qer_editorimage textures/base_floor/a_coldstairs_04_fin.tga

	diffusemap	textures/base_floor/a_coldstairs_d04_fin.tga
	specularmap	textures/base_floor/a_coldstairs_s04_fin.tga
       	bumpmap	        addnormals( textures/base_floor/a_coldstairs_local01.tga  , heightmap( textures/base_floor/a_coldstairs_b04_fin.tga, 3 ) )
        description  "blue gray, very slight rust"
}
