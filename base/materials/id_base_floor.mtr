material textures/base_floor/diamond05
{
	surfaceType "metal"
	qer_editorimage textures/base_floor/diamond05.tga

	{
		diffusemap	textures/base_floor/diamond05_d.tga
        bumpmap textures/base_floor/diamond05_local.tga
        specularmap textures/base_floor/diamond05_s.tga
        textureMatrix bumpMatrix {
			scale	 4, 4
		}
		textureMatrix specularMatrix {
			scale	 4, 4
		}
	}
}

