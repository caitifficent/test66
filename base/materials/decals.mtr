		blend filter

		map textures/particles/decals/crashland_shadow.tga
		red 1 * mguitable[ time * 0.5 ]
		green 0.6 * mguitable[ time * 0.5 ]
		blue 0.1 * mguitable[ time * 0.5 ]
		maskAlpha
		vertexcolor
	}
	{
