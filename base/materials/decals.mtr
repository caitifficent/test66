		map textures/particles/decals/crashland.tga
		red 1 * mguitable[ time * 0.5 ]
		green 0.6 * mguitable[ time * 0.5 ]
		blue 0.1 * mguitable[ time * 0.5 ]
		maskAlpha
		vertexcolor
	}
	decalInfo 0.7 0.7 ( 1 1 1 1 ) ( 0 0 0 0 )
}


