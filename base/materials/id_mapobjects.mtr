material models/mapobjects/camera/camlight
{

	noShadows
	twosided
	translucent
	nonsolid
	noimpact
	{
		blend add
		map models/mapobjects/camera/camlight.tga
		 rgb		camblinktable[ time * 0.6 ]
	}


}

