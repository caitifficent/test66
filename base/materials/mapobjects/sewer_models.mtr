		bumpDetailMap	 		textures/detail/bump/metal_detail01_local.tga
		specDetailMap	 		textures/detail/bump/metal_detail01_d.tga
		detailMult	12,6,0,0
	}
}

material models/mapobjects/sewer/blast_doors
{
 	renderbump -size 512 1024 -clampoutput 0 0 1 1 -aa 2 models/mapobjects/sewer/blast_doors_local.tga models/mapobjects/sewer/blast_door_hi.ase
	surfaceType	"metal"
	//noshadows
	{
		diffusemap	picmip -1	models/mapobjects/sewer/blast_doors_d.tga
		bumpmap		picmip -1	addnormals(models/mapobjects/sewer/blast_doors_local.tga, heightmap(models/mapobjects/sewer/blast_doors_h.tga, 4))
		specularmap	picmip -2	models/mapobjects/sewer/blast_doors_s.tga

		//diffuseDetailMap 		textures/detail/bump/vehicle_detail01_d.tga
		bumpDetailMap	 		textures/detail/bump/vehicle_detail01_local.tga
