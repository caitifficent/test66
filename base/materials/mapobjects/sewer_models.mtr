//main objective model

material  models/mapobjects/sewer/console
{
	surfaceType	"metal"
	noselfshadow	
	{
		program	interaction/selfillum_detailwm
		diffusemap	picmip -1	models/mapobjects/sewer/console_d.tga
		specularmap	picmip -2	models/mapobjects/sewer/console_s.tga
		bumpmap		picmip -1	models/mapobjects/sewer/console_local.tga
		selfIllumMap	picmip -1	models/mapobjects/sewer/console_glow.tga

		diffuseDetailMap 		textures/detail/bump/vehicle_detail01_d.tga
		bumpDetailMap	 		textures/detail/bump/vehicle_detail01_local.tga
		specDetailMap	 		textures/detail/bump/vehicle_detail01_d.tga
		detailMult	12,12,0,0
		detailWeightMap			models/mapobjects/sewer/console_detailmask.tga
	}
}
material  models/mapobjects/sewer/console_noshadow
{
	surfaceType	"metal"
	noshadows
	noOverlays
