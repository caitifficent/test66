material models/mapobjects/water_tower/watertower01
{
	noshadows
	surfacetype "metal"
	{
		diffusemap 	picmip -1	models/mapobjects/water_tower/watertower01_d.tga
		specularmap	picmip -2	models/mapobjects/water_tower/watertower01_s.tga
		bumpmap		picmip -2	models/mapobjects/water_tower/watertower01_local.tga
	
