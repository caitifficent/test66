			"picmip -1 addnormals(models/vehicles/edf_badger/badger_local.tga, heightmap(models/vehicles/edf_badger/badger_h.tga, 8 ) )",
			"picmip -2 models/vehicles/edf_badger/badger_s.tga",
	>
}

skin badger_fade {
	models/vehicles/edf_badger/badger	models/vehicles/edf_badger/badger_fade
}

material models/vehicles/edf_badger/badger_windows
{
  	surfaceType  "glass"
  	noSelfShadow
  	noshadows
  	translucent
  	forceoverlays
  	sort refractable

  	{
  			program trivialDetailmap
     		blend		blend
            detailMult          5,3,0,0
  			map		models/vehicles/edf_badger/windows.tga
  			mask	textures/detail/bump/concrete_detail01_d.tga
  	}
  	{
			maskcolor
			map				makealpha(textures/common/dark_gray.tga)
		}
		{
			blend		gl_dst_alpha, gl_one
			maskalpha
			program	sfx/cubemap
			red			Parm0
			green   	Parm1
			blue		Parm2
	}
}


