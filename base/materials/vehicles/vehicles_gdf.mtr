material models/vehicles/mirror
{
  	surfaceType  "glass"
  	noSelfShadow
  	noshadows
  	forceoverlays
  	sort refractable
	{
		program	interaction/basic_detailwm
		diffusemap	picmip -1	textures/common/dark_gray.tga
		specularmap	picmip -2	textures/common/gray.tga
		bumpmap		picmip -2	textures/common/gray.tga
		vertexcolor

		diffuseDetailMap		textures/detail/bump/vehicle_detail01_d.tga
		bumpDetailMap			textures/detail/bump/vehicle_detail01_local.tga
		specDetailMap			textures/detail/bump/vehicle_detail01_s.tga
		detailMult	9,9,0,0
		detailWeightMap			textures/common/gray.tga
	}
	{
		maskcolor
		map				makealpha(textures/common/gray.tga)
	}
	{
		blend		gl_dst_alpha, gl_one
		maskalpha
		program	sfx/cubemap
		red			Parm0
		green   	Parm1
		blue		Parm2
	}
}

material models/vehicles/mirror_x
{
  	surfaceType  "glass"
  	noSelfShadow
  	noshadows
  	translucent
  	forceoverlays
  	sort refractable

  	{
	maskcolor
	map				makealpha(textures/common/gray.tga)
	}
	{
		blend			gl_dst_alpha, gl_one
		maskalpha
		program	sfx/cubemap
		red			Parm0
		green   		Parm1
		blue			Parm2
	}
}

