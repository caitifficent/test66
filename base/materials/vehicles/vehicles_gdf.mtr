	GDF MCP
**************************************************************/

material models/vehicles/gdf_mcp/mcp_cockpit
{
	renderbump -size 2048 2048 -aa 1 -clampOutput 0 0 1 1 models/vehicles/gdf_mcp/mcp_cockpit_local.tga models/vehicles/gdf_mcp/mcp_cockpit_hi.ase
	sort opaqueFirst
	noshadows

	{
		program interaction/selfillum_detailwm
		diffusemap	picmip -1 partialLoad   models/vehicles/gdf_mcp/mcp_cockpit_d.tga
		specularmap	picmip -2 partialLoad   models/vehicles/gdf_mcp/mcp_cockpit_s.tga
		bumpmap		picmip -1 partialLoad   addnormals(models/vehicles/gdf_mcp/mcp_cockpit_local.tga, heightmap(models/vehicles/gdf_mcp/mcp_cockpit_h.tga, 5 ) )
		SelfIllumMap	picmip -2 partialLoad   models/vehicles/gdf_mcp/mcp_cockpit_glow.tga

		diffuseDetailMap			textures/detail/bump/vehicle_detail01_d.tga
		bumpDetailMap				textures/detail/bump/vehicle_detail01_local.tga
		specDetailMap				textures/detail/bump/vehicle_detail01_d.tga
		detailMult	8,8,0,0
		detailWeightMap				models/vehicles/gdf_mcp/mcp_cockpit_detailmask.tga
	}
}

