/*******************************
CC Console Part 1 Side
renderbump models/mapobjects/cc_console/cc_consolep1_lo.lwo
*******************************/

material models/mapobjects/cc_console/cc_consolep1

{
	surfaceType "metal"
	 unsmoothedtangents
	 renderbump -size 512 512 -trace 0.1 -aa 2 models/mapobjects/cc_console/cc_consolep1.tga models/mapobjects/cc_console/cc_consolep1_hi.lwo

	 diffusemap	textures/common/gray.tga
	 bumpmap 	models/mapobjects/cc_console/cc_consolep1.tga
	 specularmap	textures/raven/terminal/t1_metal1dark_s.tga  
}


