    noshadows
	translucent

	{
		maskcolor
		map makealpha(models/items/powerups/berserkermask.tga)

	}
	{
	        blend gl_dst_alpha, gl_one
			//maskalpha
		    map models/items/powerups/berserkerfx.tga
			translate	time * 0.1 , time * 2
	}
	{
		blend add
		map models/items/powerups/berserkerfx2.tga
	}

}

