material particles/flash/flame_ember01
{
	qer_editorimage textures/particles/flash/ember_mid.tga
	sort nearest
	{
		blend blend
		map clamp textures/particles/flash/ember_mid.tga
		//alpha 0
		maskalpha
		vertexcolor
	}
	{
		blend add
		color 0.5, 0.5, 0.5, 0.5
		map clamp textures/particles/flash/ember_mid.tga
		//alpha 0
		maskalpha
		vertexcolor
	}
}

//was material particles/penta/flames4
material particles/flash/licking_flames01 {
	{
		blend add
		map zeroclamp textures/particles/fire/flame5_strip.tga
		maskalpha
		vertexcolor
		useTemplate templates/materials/animate<32, 2.5> 
	}
}

