material particles/flash/flare
{
	qer_editorimage models/weapons/shotgun/shotgun_mflash.tga
	{
			blend		add
			map		zeroclamp textures/effects/weapons/muzzleflash_1a.tga
			maskalpha
			vertexcolor
			useTemplate templates/materials/animate< 4, 0.1 >
	}
}



