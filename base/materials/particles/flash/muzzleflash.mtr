material particles/flash/3_pointflash
{
	qer_editorimage textures/effects/weapons/3point_flash.tga
	nonsolid
	sort nearest
	noshadows
	//twoSided
	{
			blend		add
			map zeroclamp textures/effects/weapons/3point_flash.tga
			maskalpha
			vertexcolor
			//useTemplate templates/materials/animate< 4, 0.1 >
	}
}


