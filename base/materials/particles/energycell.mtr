material textures/particles/energycell_vortex01
{
	NoShadows
	translucent
	nonSolid
	sort far
 
   	{
   		blend blend
   		map clamp textures/particles/energycell_vortex01.tga
   		rotate time * -0.05
   		vertexcolor
  	}
}

