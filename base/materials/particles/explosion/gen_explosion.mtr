material particles/explosion/explode01
{
	qer_editorimage models/weapons/shotgun/shotgun_mflash.tga
	sort nearest
	//deform sprite
	{
		blend add
		map clamp textures/particles/explosion/explode01.tga
		useTemplate templates/materials/animate< 16, 0.5 >
		alpha 1
		maskalpha
	}
}


material textures/effects/weapons/flash
