	qer_editorimage textures/effects/weapons/flash.tga
//	sort nearest
	//deform sprite
	{
		blend add
		map clamp textures/effects/weapons/flash.tga
//		alpha 1
		vertexcolor
		maskalpha
	}
}


material particles/explosion/explode02
{
	qer_editorimage models/weapons/shotgun/shotgun_mflash.tga
	sort nearest
	//deform sprite
	{
		blend add
		map textures/particles/explosion/explode02.tga
		scale 1 / 8, 1
		scroll table8[ time * 1 ] , 0
		alpha 1
		maskalpha
	}
}


