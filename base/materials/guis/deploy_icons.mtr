
#include <materials/guis/targeting.include>

material guis/assets/deploy/icons/gdf_am
{
	sort gui
	{
		blend blend
		vertexColor
		map clamp nopicmip guis/assets/deploy/icons/gdf_am.tga
	}
}


material guis/assets/deploy/icons/gdf_am_desat {
	target_desat( "gdf_am" )
}

