material models/structures/edf/cc/interior/cc_interior_1
{
	surfacetype "metal"
	{
	program interaction/selfillum
	diffusemap	models/structures/edf/cc/interior/cc_interior_1_d.tga
	bumpmap		models/structures/edf/cc/interior/cc_interior_1_local.tga
	specularmap	models/structures/edf/cc/interior/cc_interior_1_s.tga
	selfillummap	models/structures/edf/cc/interior/cc_interior_1_glow.tga
	}
}

material models/structures/edf/cc/interior/cc_interior_2
