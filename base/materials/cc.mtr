material models/structures/edf/cc/interior/cc_interior_4
{
	noshadows
	twosided
	nonsolid
	DECAL_MACRO
	useTemplate templates/windowglass_mask_specular_cc <
		models/structures/edf/cc/interior/cc_interior_4_d.tga,
		models/structures/edf/cc/interior/cc_interior_4_s.tga,
		models/structures/edf/cc/interior/cc_interior_4_local.tga,
		models/structures/edf/cc/interior/cc_interior_4_mask.tga,
		2, 1.0, 0.3
	>
	{
		blend blend
		depthfunc less
		map models/structures/edf/cc/interior/cc_interior_4_dirt.tga
		
	}
}

material models/structures/edf/cc/interior/cc_interior_5
{
	nonsolid
	translucent
	{
		blend	filter
		map	models/structures/edf/cc/interior/cc_interior_5_d.tga
	}
}