		diffusemap	textures/base_trim/factory_interiorwall02_trim_d.tga
		specularmap	textures/base_trim/factory_interiorwall02_trim_s.tga
		bumpmap 	textures/base_trim/factory_interiorwall02_trim_local.tga
		vertexcolor
	}
}

material textures/base_trim/grey_trim01
{
	surfacetype "metal"
	{
		diffusemap	textures/base_trim/grey_trim01_d.tga
		specularmap	textures/base_trim/grey_trim01_s.tga
		bumpmap 	textures/base_trim/grey_trim01_local.tga
		vertexcolor
		
		diffuseDetailMap		textures/detail/bump/metal_detail01_d.tga
		bumpDetailMap			textures/detail/bump/metal_detail01_local.tga
		specDetailMap			textures/detail/bump/metal_detail01_d.tga
		detailMult 8,1,0,0 
	}
}

material texturesheets/volcano/ceiling_light
{
		noshadows
		noselfshadow
		//nonsolid
		//qer_editorimage texturesheets/volcano/ceiling_light_d.tga
		diffusemap	texturesheets/volcano/ceiling_light_d.tga
		//specularmap     textures2/lab/lab_ceiling_light_on_s.tga
		//bumpmap 	textures2/lab/lab_ceiling_light_on_local.tga
	{	
		blend add
		map		texturesheets/volcano/ceiling_light_glow_d.tga
