material textures2/metal/railing01
{
	nonsolid
	playerclip
	flyerhiveclip
	noimpact
	noShadows
	twosided
{
	bumpmap		 textures2/refinery/railing01_local.tga
	diffusemap	picmip -1 textures2/refinery/railing01_d.tga
	specularmap	picmip -1 textures2/refinery/railing01_s.tga
	alphaTest 0.5
}
}

material textures2/ground/tar_roof01
{
	{
	diffusemap	picmip -1	textures2/refinery/tar_roof01_d.tga
	specularmap	picmip -2	textures2/refinery/tar_roof01_s.tga
	bumpmap		picmip -1	textures2/refinery/tar_roof01_local.tga
	
	diffuseDetailMap		textures/detail/bump/concrete_detail01_d.tga
	specDetailMap			textures/detail/bump/concrete_detail01_d.tga
	bumpDetailMap			textures/detail/bump/concrete_detail01_local.tga
	detailMult	8,8,0,0
	}
}

