material textures/raven/terminal/tunlight2
{
	bumpmap			picmip -1	addnormals ( textures/raven/terminal/tunlight_local, heightmap ( textures/raven/terminal/tunlight_h, 1 ) )
	diffusemap		picmip -1	textures/raven/terminal/tunlight_d
	specularmap		picmip -2	textures/raven/terminal/tunlight_s
	{
		blend		GL_ONE, GL_ONE
		map			textures/raven/terminal/tunlight2_g
		red			Parm0  
		green		Parm1  
		blue		Parm2  
	}
}

