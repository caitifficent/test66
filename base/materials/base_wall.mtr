material textures/concrete/plaster
{
	qer_editorimage textures/concrete/plaster_d.tga
	diffusemap	textures/concrete/plaster_d.tga
	specularmap	picmip -2	textures/concrete/concrete01_s.tga
	bumpmap		heightmap(textures/concrete/plaster_h.tga, 2)
}

material textures/concrete/plaster_bricks
{
	qer_editorimage textures/concrete/plaster_bricks_d.tga
	diffusemap	textures/concrete/plaster_bricks_d.tga
	specularmap	picmip -2	textures/concrete/concrete01_s.tga
	//bumpmap	heightmap(textures/concrete/plaster_h.tga, 2)
	bumpmap		addnormals(textures/concrete/plaster_bricks_local.tga, heightmap(textures/concrete/plaster_h.tga, 2))
}

