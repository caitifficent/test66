material textures/base_wall/melting_metal
{
    { 
        diffusemap 	textures/raven/medlabs/tubearmsplatform_platformtopbottom_d.tga   
        bumpmap 	textures/raven/medlabs/tubearmsplatform_platformtopbottom_local.tga   
        specularmap 	textures/raven/medlabs/tubearmsplatform_platformtopbottom_s.tga   
        vertexColor
    }
    
    {
        diffusemap 	textures/sfx/lfwall27alava_d
	bumpmap 	textures/sfx/lfwall27alava_local
        specularmap	textures/sfx/lfwall27alava_s
        inverseVertexColor
    }
    {
        blend		gl_one , gl_one
    	map		textures/sfx/radiogoop_red
    	rotate		time*.00015
    	inverseVertexColor
    }
    {
        blend		gl_one , gl_one
        map		textures/sfx/radiogoop_red
        rotate		time*-.00015
        translate	time*.002,1
    	inverseVertexColor
    }
}

material textures/base_wall/melting_metal_2
{
	surfacetype "metal"
	qer_editorimage	textures/base_wall/conc_metal_1_d.tga
	rendermodelclip
	noselfshadow
    { 
        diffusemap		picmip -1	textures/base_wall/conc_metal_1_d.tga  
        bumpmap			picmip -1	textures/base_wall/conc_metal_1_local.tga
        specularmap		picmip -2	textures/base_wall/conc_metal_1_s.tga
        VertexColor
        
    /*    diffuseDetailMap			textures/detail/bump/metal_detail01_d.tga
	specDetailMap				textures/detail/bump/metal_detail01_d.tga
	bumpDetailMap				textures/detail/bump/metal_detail01_local.tga
	detailMult		4,4,0,0*/
    }
    {
		if cvar (r_megaDrawMethod == 0)
     	diffusemap          	picmip -1	textures/sfx/lfwall27alava_d
     	specularmap         	picmip -2	textures/sfx/lfwall27alava_s
     	bumpmap             	picmip -1	textures/sfx/lfwall27alava_local
	inverseVertexColor
		
	scale 3, 4 
	rotate .35
    }
    {
        blend			add
       	map			picmip -1	textures/sfx/radiogoop_red
        rotate			time*.00015
        rotate .35
        scale 3, 4
        inverseVertexColor
    }
    {
        blend			add
        map			picmip -1	textures/sfx/radiogoop_red
        rotate			time*-.00015
        translate		time*.002,4
        scale 3, 4
       	inverseVertexColor
    }
    
    /*{
    	program	interaction/blended
		diffusemap		picmip -1	textures2/floors/concrete_tiles01_d.tga   
		specularmap		picmip -1	textures2/floors/concrete_tiles01_s.tga
		bumpmap 		picmip -2	textures2/floors/concrete_tiles01_local.tga   
		diffusemap2		picmip -1	textures2/concrete/rubble01_d.tga
		bumpmap2		picmip -1	textures2/concrete/rubble01_local.tga 
		specularmap2	picmip -2	textures2/concrete/rubble01_s.tga
		inversevertexcolor
	}*/
} 

