material models/vegetation/trees/ash/ash01a
{
 	nonsolid
 	noshadows
        twoSided

 	{
 		alphatocoverage
 		program		interaction/foliage
  		diffusemap	clamp models/vegetation/trees/ash/ash01a_d.tga
		//bumpmap	models/andrew/tree02/tree02a_local.tga
