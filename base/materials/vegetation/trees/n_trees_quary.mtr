///ANDREW///

material models/vegetation/trees/quarry/quarry_branches
{
	nonsolid
	noshadows
	twoSided
 	{
		//alphatocoverage
		program interaction/foliage
		diffusemap	clamp	models/vegetation/trees/n_trees_quary/quarry_branches_01_d.tga
		vertexcolor
		alphaTest	0.25
 		
		useTemplate wind/animatematrix_left< 0.12, 0.002 > // Speed, strength	
	}
}

material models/vegetation/trees/quarry/quarry_bark
{
	nonsolid
	noshadows
	{
		diffusemap				models/vegetation/trees/n_trees_quary/quarry_bark_01_d.tga
		bumpmap		picmip -1	models/vegetation/trees/n_trees_quary/quarry_bark_01_local.tga
		specularmap	picmip -1	models/vegetation/trees/n_trees_quary/quarry_bark_01_s.tga
		alphaTest	0.25
		vertexcolor
		
		bumpDetailMap			textures/detail/bump/bark_detail01_local.tga
		specDetailMap			textures/detail/bump/bark_detail01_d.tga
		detailMult 10,10,0,0 
	}
}

