material models/vegetation/trees/pine/pine01a
{
 	nonsolid
 	noshadows
        twoSided

 	{
 		alphatocoverage
 		program		interaction/foliage
  		diffusemap 	clamp models/vegetation/trees/pine/pine01a_d.tga
 		alphaTest 	0.25
 		//bumpmap	models/andrew/tree02/tree02a_local.tga
 		
		useTemplate wind/animatematrix< 0.15, 0.001 >
 	}
}



