material models/andrew/newtree/newtree01_leaves
{
 
 		

 	nonsolid
 	noshadows
    twoSided

 	{
 		alphatocoverage
  		program interaction/foliage
  		diffusemap clamp models/andrew/newtree/newtree02.tga
 		//bumpmap clamp models/andrew/newtree/newtree02_local.tga
 		alphaTest 	0.25
 		
		useTemplate wind/animatematrix< 0.15, 0.005 > // Speed, strength	
 	}
	

}

