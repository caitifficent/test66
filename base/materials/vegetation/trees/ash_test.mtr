material models/vegetation/trees/ash/ash_large_bark
{
	noshadows
	{
		diffusemap	picmip -1	models/vegetation/trees/ash/ash_large_bark_01_d.tga
		specularmap	picmip -2	models/vegetation/trees/ash/ash_large_bark_01_s.tga
