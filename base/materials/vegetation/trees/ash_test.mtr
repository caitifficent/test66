material models/vegetation/trees/ash/ash_leaves
{
 	nonsolid
 	noshadows
    twoSided
 	{
 		alphatocoverage
  		program interaction/foliage
  		diffusemap	picmip -1	clamp	models/vegetation/trees/ash/ash_leaves_01_d.tga
 		alphaTest 	0.25
 		
		useTemplate wind/animatematrix_left< 0.12, 0.002 > // Speed, strength	
 	}
}

material models/vegetation/trees/ash/ash_bark
{
	nonsolid
	noshadows
	{
		program	interaction/basic_detail_alphatest
		diffusemap				models/vegetation/trees/ash/ash_bark_01_d.tga
		bumpmap		picmip -1	models/vegetation/trees/ash/ash_bark_01_local.tga
		specularmap	picmip -2	models/vegetation/trees/ash/ash_bark_01_s.tga
		alphaTest	0.25
		vertexcolor
		
		diffuseDetailMap		textures/detail/bump/bark_detail01_d.tga
		bumpDetailMap			textures/detail/bump/bark_detail01_local.tga
		specDetailMap			textures/detail/bump/bark_detail01_d.tga
		detailMult	8,12,0,0
	}
}

material models/vegetation/trees/ash/ash_branches
{
	nonsolid
	noshadows
	twoSided
	{
 		alphatocoverage
		program interaction/foliage
		diffusemap	picmip -2	clamp	models/vegetation/trees/ash/ash_branches_01_d.tga
		bumpmap		picmip -1	clamp	models/vegetation/trees/ash/ash_branches_01_local.tga
		specularmap	picmip -2	clamp	models/vegetation/trees/ash/ash_branches_01_s.tga
		alphaTest	0.17

		useTemplate wind/animatematrix< 0.15, 0.0005 > // Speed, strength
	}
}

