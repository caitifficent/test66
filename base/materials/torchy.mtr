material models/mapobjects/ruins/stonehut_new 
{   
	renderbump -size 512 512 -colormap -aa 2 models/mapobjects/ruins/stonehut_new.tga models/mapobjects/ruins/stonehut_square_high.lwo
	surfaceType	"stone"

	diffusemap      models/mapobjects/ruins/stonehut_reddish_d.tga   
	specularmap     models/mapobjects/ruins/stonehut_yellow_s.tga   
	bumpmap         models/mapobjects/ruins/stonehut_yellow_local.tga   
}

material textures/valley/conc12torubble
{
	
	qer_editorimage	textures/concrete/concrete12_d.tga
	{
		program	interaction/blended
		diffusemap textures/concrete/concrete12_d.tga
		bumpmap textures/concrete/concrete12_local.tga
		specularmap textures/concrete/concrete12_s.tga
		diffusemap2 texturesheets/ghost_town/rubble01_d.tga
		bumpmap2 texturesheets/ghost_town/rubble01_local.tga
		specularmap2 texturesheets/ghost_town/rubble01_s.tga
		inversevertexcolor
	}
}

material textures/salvage/conc_burn_blend
{
	
	qer_editorimage	textures/concrete/concrete12_d.tga
	{
		program	interaction/blended
		diffusemap	textures2/concrete/concrete_dirty_generic_8_d.tga
		specularmap     textures2/concrete/concrete_dirty_generic_8_s.tga
		bumpmap 	textures2/concrete/concrete_dirty_generic_8_local.tga
		diffusemap2 textures/raven/common_walls/roger_trough_d.tga
		bumpmap2 textures/raven/common_walls/roger_trough_local.tga
		specularmap2 textures/raven/common_walls/roger_trough_s.tga
		inversevertexcolor
	}
}

material textures/salvage/conc_burn
{
	
	qer_editorimage	textures/raven/common_walls/roger_trough_d.tga
	{
		program	interaction/selfillum
		diffusemap	textures/raven/common_walls/roger_trough_d.tga
		specularmap	textures/raven/common_walls/roger_trough_s.tga
		bumpmap		addnormals(textures/raven/common_walls/roger_trough_local.tga, heightmap(textures/raven/common_walls/roger_trough_h.tga, 5 ) )
		selfIllumMap	textures/raven/common_walls/roger_trough_d.tga
	}
}