material textures/refinery/rocktocavemud
{
	
	qer_editorimage	textures/rock/cracked_rock3_ver2_gm_d.tga
	{
		program	interaction/blend
		diffusemap textures/rock/cracked_rock3_ver2_gm_d.tga
		bumpmap addnormals (textures/rock/cracked_rock3_ver2_local.tga, heightmap (textures/rock/cracked_rock3_ver2_h.tga, 5))
		diffusemap2	textures2/megagen/dirt/temperate/dirt_bank_wateredge_4t1_d.tga
		bumpmap2 	textures2/megagen/dirt/temperate/dirt_bank_wateredge_4t1_local.tga
	}
}

