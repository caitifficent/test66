material textures/torchy/razorwire
{
	qer_editorimage textures/torchy/razorwire_d.tga

	noshadows
	twoSided
	nonsolid
	playerclip // need a better solution which only slows players down significantly
	flyerhiveclip

	{
		diffusemap textures/torchy/razorwire_d.tga
		alphaTest 0.5
		bumpmap textures/torchy/razorwire_local.tga
		specularmap textures/torchy/razorwire_s.tga
	} 
}

