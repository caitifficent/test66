material textures/zombie/fluid_blue_bubble
{	
	qer_editorimage
	noShadows
	translucent
	nonSolid
	twoSided	
		
	{
		blend add	
		map textures/zombie/fluid_blue_bubble1.tga
		scroll 0, time * .1
	}

	{ 
		blend add
		map textures/zombie/fluid_blue_bubble2.tga
		scroll	0, time * .5
	}
	
	{
		blend add
		map textures/zombie/fluid_blue_bubble3.tga
		scroll 0, time * .3
	}

}

