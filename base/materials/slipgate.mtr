		specularmap	picmip -2	textures/rock/sharprock2_s.tga
		bumpmap		picmip -1	addnormals( textures/rock/sharprock2_local.tga  , heightmap( textures/rock/skyrock3_b.tga, 4 ) )
		 	
		vertexcolor
	}
}

