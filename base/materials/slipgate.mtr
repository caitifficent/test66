material textures/dirt/dry_earth_rubble
{
	
	qer_editorimage	textures/dirt/dry_earth_editor.tga
	{
		diffusemap	textures/dirt/dry_earth_d.tga
		specularmap 	textures/rock/sharprock2_s.tga
		bumpmap		addnormals( textures/rock/sharprock2_local.tga  , heightmap( textures/rock/skyrock3_b.tga, 4 ) )
		 	
		vertexcolor
	}
}

material textures/dirt/dry_earth_rubble_noshadows
