material models/mapobjects/slipgate/barrel_01
{
	noshadows
	{
		diffusemap			models/mapobjects/slipgate/barrel_01_d.tga
		specularmap			models/mapobjects/slipgate/barrel_01_s.tga
		bumpmap				models/mapobjects/slipgate/barrel_01_local.tga
	
		diffuseDetailMap	textures/detail/bump/vehicle_detail01_d.tga
