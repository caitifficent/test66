material textures/base_light/reactorlight1
{
	surfaceType "metal"
	qer_editorimage		textures/base_light/reactorlight1.tga
	bumpmap			textures/base_light/reactorlight1_local.tga
	diffusemap		textures/base_light/reactorlight1_d.tga
	specularmap		textures/base_light/reactorlight1_s.tga
	{
		blend 		add
		map 		textures/base_light/reactorlight1_add.tga
		red		parm0 * 2
		green		parm1 * 2
		blue		parm2 * 2
	}
}

material textures/base_light/reactorlight1_flickering
{
	surfaceType "metal"
	qer_editorimage		textures/base_light/reactorlight1.tga
	bumpmap			textures/base_light/reactorlight1_local.tga
	diffusemap		textures/base_light/reactorlight1_d.tga
	specularmap		textures/base_light/reactorlight1_s.tga
	{
		blend 		add
		map 		textures/base_light/reactorlight1_add.tga
		red 	( round_flicker_2 [Parm4 + (time * (0.1 * Parm3)) ]) * Parm0
		green 	( round_flicker_2 [Parm4 + (time * (0.1 * Parm3)) ]) * Parm1
		blue	( round_flicker_2 [Parm4 + (time * (0.1 * Parm3)) ]) * Parm2
	}
}

