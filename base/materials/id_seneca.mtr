material textures/particles/dustcloud_opaque
{
	qer_editorimage		textures/particles/dustcloud
	nonsolid
	noshadows
	{
		blend			blend
		map				textures/particles/dustcloud_opaque
		vertexColor
		maskAlpha
		//color sun_r, sun_g, sun_b, 1
		red desat_sun_r * lightscale * 0.4
		green desat_sun_g * lightscale * 0.4
		blue desat_sun_b * lightscale * 0.4
	}
}

