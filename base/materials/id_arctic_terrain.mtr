material textures/arctic_snow/powder01_powder02_blend
{
	{
		blend 		bumpmap
		map		textures/arctic/snow/powder_01_n.tga
	}
	{
		blend 		diffusemap
		map		textures/arctic/snow/powder_01_d.tga
		vertexColor
	}
	{
		blend 		specularmap
		map		textures/arctic/snow/powder_01_s.tga
		vertexColor
	}
	{
		blend		bumpmap
		map		textures/arctic/snow/powder_02_n.tga
	}
	{
		blend		diffusemap
		map		textures/arctic/snow/powder_01_d.tga
		inverseVertexColor
	}
	{
		blend		specularmap
		map		textures/arctic/snow/powder_02_s.tga
		inverseVertexColor
	}
}

