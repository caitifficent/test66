
material fogs/waterFog
{
	fogLight refractable		// tell the engine it is fog and not a light
	noShadows
	{
		fogMap		_fog
		fogEnterMap	_fogEnter
		red			parm0
		green		parm1
		blue		parm2
		alpha		100
		specularPower	10
	}
}

