
/**************************************************************
	Supply Pack
**************************************************************/

material models/techkits/supply_pack/supply_pack
{
	renderbump -size 1024 512 -clampOutput 0 0 1 1 -colormap models/techkits/supply_pack_local.tga models/techkits/supply_pack_hi.lwo
	diffusemap	models/techkits/supply_pack_d.tga
	specularmap	models/techkits/supply_pack_s.tga
	bumpmap 	addnormals( models/techkits/supply_pack_local.tga, heightmap( models/techkits/supply_pack_h.tga, 5 ) )
}


