// -----------------------

skin reactiveCamo {
	*	textures/effects/reactiveCamoSkin
}

material textures/effects/reactiveCamoSkin
{
	surfaceType "flesh"
	noShadows
	noSelfShadow
	translucent		// don't write during the fill depth buffer phase
	sort postprocess
	{
		program heatHaze
		deformScroll 0 , time * 0.05
		deformMagnitude 0.1
		bumpMap picmip -1	textures/hell/lava1_local.tga
		maskAlpha
	}
}

material textures/sfx/thirdeyemonitor
{
	sort gui
	/*{
		program	tiles.vfp
		vertexParm		0		160
		vertexParm		1		0.43 + ( sinTable[ time * 0.15 ] * cosTable[ time * 0.25 ] ) * 0.2
		fragmentMap		0		_currentRender
	}*/
	{
		map guis/assets/common/gui_scanlines
		blend add
		translate		0, time * -0.1
	}
	{
		map guis/assets/qet/resupply_bg
		blend filter
	}
}
