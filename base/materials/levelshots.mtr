// ==========================================================
// Campaigns
// ==========================================================

material levelshots/campaigns/africa { 
	sort gui
	{
		blend blend
		vertexColor
		map nopicmip clamp	levelshots/campaigns/africa.tga
	}
	{
		blend add
		map	guis/assets/mainmenu/scanlines.tga
		rgb 0.5
		scale 16, 14
	}
	{
		blend filter
		vertexColor
		map highquality clamp	levelshots/campaigns/gradient.tga
	}
}

