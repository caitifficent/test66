

// ==========================================================
// Generic Assets
// ==========================================================

// ==========================================================
// used for the server browser
// level shot images are directly loaded from disk and uploaded
// over levelshots/__scratch.tga
// ==========================================================
material levelshots/scratch {
	sort gui
	{
		blend blend
		map clamp nopicmip forceHighQuality levelshots/__scratch.tga
		vertexColor
	}
}

material levelshots/campaigns/scratch {
	sort gui
	{
		blend blend
		map clamp nopicmip forceHighQuality levelshots/campaigns/__scratch.tga
		vertexColor
	}
}

material levelshots/generic { 
	sort gui
	{
		blend blend
		vertexColor
		map clamp nopicmip	"levelshots/generic"
	}
}

