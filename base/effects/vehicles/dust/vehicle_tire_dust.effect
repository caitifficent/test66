			duration	0.5,1.5
			persist
			material	"textures/particles/smoke_test1"
			gravity		-0.02,-0.01
			trailRepeat	1

			start
			{
				position { line 0,0,-8,-16,0,-8 }
				velocity { box -20,-4,-4,20,4,4 }
				size { box 16,16,32,32 }
				tint { line 0.14902,0.14902,0.14902,0.74902,0.709804,0.65098 }
				fade { line 0.2,0.6 }
				rotate { box 0,1 }
			}

			motion
			{
				tint { envelope linear }
				fade { envelope linear }
			}

			end
			{
				tint { line 0.0941177,0.0941177,0.0941177,0.635294,0.517647,0.392157 }
			}
