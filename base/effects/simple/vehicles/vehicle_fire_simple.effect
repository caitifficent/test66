effect effects/vehicles/vehicle_flames_small3_simple
{
	size	161

	cutOffDistance	4000

	emitter "smoke"
	{
		duration	1,1
		count		14,14
		attenuateEmitter
		attenuation	250,250

		sprite
		{
			duration	3,4
			persist
			material	"particles/penta/hornetsmoke2"
		parentvelocity
			windDeviationAngle 180.000000
			gravity		-0.04,-0.03
			trailRepeat	1

			start
			{
				position { sphere 20,-10,50,10,10,10 }
				velocity { box -10,-10,5,10,10,15 }
				friction { point 1,0,0 }
				size { line 10,10,20,20 }
				tint { point 0.564706,0.282353,0 }
				windStrength { point -0.025 }
				rotate { box -0.125,0.125 }
			}

			motion
			{
				size { envelope jrad_concavefade }
				tint { envelope rr2_convexfade_start_at_1 count 2,2,2 offset 0.5,0.5,0.5 }
				fade { envelope rr2_convexfade_start_at_1 offset 0.025 }
				rotate { envelope convexfade }
			}

			end
			{
				size { line 50,50,70,70 }
				tint { point 0.137255,0.137255,0.137255 }
				rotate { box -0.527778,0.527778 relative }
			}
			lineHit
		}
	}
	emitter "flames"
	{
		duration	1,1
		count		4,6
		locked
		attenuateEmitter
		attenuation	0,250

		sprite
		{
			duration	0.5,1.5
			persist
			material	"particles/penta/flames2"
			gravity		-0.03,-0.02
			trailRepeat	1
			trailType	burn
			trailTime	1,1
			trailCount	1,1

			start
			{
				position { sphere 20,-20,15,10,20,50 cone }
				velocity { box -5,-5,-5,5,5,5 }
				size { line 8,15,16,30 }
				tint { line 1,1,0.501961,1,0.501961,0.247059 }
				fade { point 0 }
			}

			motion
			{
				tint { envelope rr2_convexfade_start_at_1 }
				fade { envelope fastinslowout }
				rotate { envelope linear }
			}

			end
			{
				fade { point 1 }
				rotate { point 0 relative }
			}
			lineHit
		}
	}
	emitter "glow"
	{
		duration	1,1
		count		2,2
		locked

