effect effects/weapons/spikes_raise_simple
{
	size	20

	cutOffDistance	100

	spawner "centre sparks"
	{
		count		1,1
		locked
		orientateIdentity

		sprite
		{
			duration	0.2,0.2
			blend	add
			material	"textures/particles/sparks/sparkflash_strip01"
			numFrames 4
			trailRepeat	1

			start
			{
				size { point 4,4 }
				tint { point 0.921569,0.74902,0.501961 }
				fade { line 0.1,0.3 }
				offset { point 0,-7,-4 }
				rotate { box -0.5,0.5 }
			}

			motion
			{
				size { envelope linear }
			}

			end
			{
				size { point 7,7 }
			}
			lineHit
		}
	}
	spawner "centre sparks2"
	{
		count		1,1
		locked
		orientateIdentity

		sprite
		{
			duration	0.2,0.2
			blend	add
			material	"textures/particles/sparks/sparkflash_strip01"
			numFrames 4
			trailRepeat	1

			start
			{
				position { point -5,0,0 }
				size { point 3,3 }
				tint { point 1,0.74902,0.509804 }
				fade { line 0.1,0.3 }
				offset { point 0,-7,-4 }
				rotate { box -0.5,0.5 }
			}

			motion
			{
				size { envelope linear }
			}

			end
			{
				size { point 6,6 }
			}
			lineHit
		}
	}
}

