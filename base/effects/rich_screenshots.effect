		}
	}
	emitter "Flame"
	{
		duration	1,1
		count		6,6

		sprite
		{
			duration	2,2
			persist
			material	"particles/penta/flames3"
			trailRepeat	1

			start
			{
				position { point -50,-50,0 }
				acceleration { box 0,0,20,0,0,30 }
				size { box 30,30,50,50 }
				fade { point 0 }
			}

			motion
			{
				size { envelope jrad_alphain }
				tint { envelope jrad_concavefade }
				fade { envelope bfgguy_vtable }
