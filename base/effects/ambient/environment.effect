effect effects/ambient/fallingleaves01
{
	size	181

	emitter "unnamed0"
	{
		duration	3,5
		count		10,10

		sprite
		{
			duration	7,15
			material	"particles/debris/debris_leaves03"
			numFrames 4
			windDeviationAngle 50.000000
			trailRepeat	1

			start
			{
				size { line 3,3,5,5 }
				fade { point 0 }
				offset { spiral -50,-100,-100,100,100,100,1000 }
				windStrength { line 0,1 }
				rotate { box 13,-13 }
			}

			motion
			{
				fade { envelope shamus_fastinslowout }
				offset { envelope concavefade count 1.5,1.5,1.5 }
				rotate { envelope linear }
			}

			end
			{
				fade { point 1 }
				offset { spiral -200,-100,0,-550,100,100,1000 }
				rotate { box -13,13 }
			}
			lineHit
		}
	}
	delay "unnamed1"
	{
		duration	10,10
	}
}

