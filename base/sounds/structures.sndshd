//////////////////////////
// Structure Explosions //
//////////////////////////

sound strogg_base {
	minDistance	35
	maxDistance	150
	volume		-3
	looping
	
	sounds/structures/domihub/ambience.wav
}

sound structure_explode {

	volume 5
	maxDistance 250
	no_occlusion
	omnidirectional
	shakes 0.1

	sounds/structures/structure_explode.wav
}


