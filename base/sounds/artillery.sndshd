sound sounds/deployables/artillery/fire {

	shakes 0.01
	maxDistance 100
	no_occlusion

	sounds/deployables/artillery/fire_01.wav
	sounds/deployables/artillery/fire_02.wav
	sounds/deployables/artillery/fire_03.wav
}

sound sounds/deployables/artillery/fire/far {

	volume -5
	farDistance 75
	maxDistance 150
	no_occlusion

	sounds/deployables/artillery/fire_far.wav
}

sound sounds/deployables/artillery/reload {

	volume -10

	sounds/deployables/artillery/reload.wav
}

sound sounds/deployables/artillery/explode {

	shakes 0.05
	minDistance	15
	maxDistance 100
	
	occlude_once

	sounds/deployables/artillery/artillery_impact_01.wav
	sounds/deployables/artillery/artillery_impact_02.wav
	sounds/deployables/artillery/artillery_impact_03.wav
}

