	maxDistance 100

	sounds/deployables/artillery/fly_01.wav
	sounds/deployables/artillery/fly_02.wav
	sounds/deployables/artillery/fly_03.wav
	sounds/deployables/artillery/fly_04.wav
	sounds/deployables/artillery/fly_05.wav
	sounds/deployables/artillery/fly_06.wav
	
