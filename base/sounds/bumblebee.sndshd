
///////////////
// Bumblebee //
///////////////

sound sounds/vehicles/bumblebee/rotor_start {

	maxDistance 100
	antiPrivate
	playOnce

	looping
	leadin
	sounds/vehicles/bumblebee/rotor_start.wav
	sounds/vehicles/bumblebee/rotor_loop.wav
}

sound sounds/vehicles/bumblebee/rotor_stop {

	maxDistance 100
	antiPrivate

	sounds/vehicles/bumblebee/rotor_stop.wav
}

sound sounds/vehicles/bumblebee/turbine_start {

	maxDistance	100
	antiPrivate
	playOnce

	looping
	leadin
	sounds/vehicles/bumblebee/turbine_start.wav
	sounds/vehicles/bumblebee/turbine_loop.wav
}

sound sounds/vehicles/bumblebee/turbine_stop {

	maxDistance	100
	antiPrivate

	sounds/vehicles/bumblebee/turbine_stop.wav
}

sound sounds/vehicles/bumblebee/turbo/start {

	volume -5
	maxDistance 100

	looping
	leadin
	sounds/vehicles/bumblebee/turbo_start.wav
	sounds/vehicles/bumblebee/turbo_loop.wav
}


